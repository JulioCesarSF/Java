package br.com.fiap.teste;

import java.util.Scanner;

public class Exemplo02 {

	public static void main(String[] args) {

		try (Scanner t = new Scanner(System.in)) {

			int[][] n = new int[4][4];

			for (int i = 0; i < 4; i++) {

				for (int j = 0; j < 4; j++) {
					
					n[i][j] = t.nextInt();

				}

			}

		} catch (Exception e) {
			System.out.println("Erro:" + e);
		}

	}

}
